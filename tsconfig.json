{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node",
    "noEmit": true,
    "target": "esnext",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "module": "CommonJS",
    "strict": false,
    "resolveJsonModule": true,
  }
}
